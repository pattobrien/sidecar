    "version": "0.2.0",
    "configurations": [
        {
            "name": "Sidecar Lint Debugger",
            "program": "examples/my_analyzed_codebase/.dart_tool/sidecar/debug.dart",
            "request": "launch",
            "type": "dart",
            "args": [
                "--enable-vm-service",
                "--debug",
            ]
        },
        {
            "name": "Benchmark Debugger",
            "program": "examples/benchmark/bin/debug.dart",
            "request": "launch",
            "type": "dart",
            "args": [
                "--enable-vm-service"
            ]
        },
        {
            "name": "Sidecar Debugger from Port",
            "program": "/Users/pattobrien/Development/sidecar/packages/sidecar/bin/debug.dart",
            // "program": ".sidecar/sidecar_analyzer_plugin/bin/debug.dart",
            "request": "attach",
            "type": "dart",
            "vmServiceUri": "http://127.0.0.1:9990/ws",
            "args": [
                "--enable-vm-service"
            ],
        }
    ],
}